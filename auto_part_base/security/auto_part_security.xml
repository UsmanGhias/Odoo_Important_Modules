<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.module.category" id="module_category_auto_part">
        <field name="name">Auto Part</field>
        <field name="description">User access level for Auto Parts</field>
        <field name="sequence">3</field>
    </record>
    <record id="group_gen_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="auto_part_base.module_category_auto_part"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
    <record id="group_admin_user" model="res.groups">
        <field name="name">Admin</field>
        <field name="implied_ids" eval="[(4, ref('auto_part_base.group_gen_user'))]"/>
        <field name="category_id" ref="auto_part_base.module_category_auto_part"/>
        <field name="users" eval="[(4, ref('base.user_admin'))]"/>
    </record>
</odoo>