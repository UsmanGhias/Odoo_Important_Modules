<odoo>
    <record id="account_reports.aged_receivable_report" model="account.report">
        <field name="column_ids">
            <record id="aged_receivable_report_po_number_ref" model="account.report.column">
                <field name="name">PO Number</field>
                <field name="expression_label">po_number_ref</field>
                <field name="figure_type">string</field>
                <field name="sortable" eval="True"/>
                <field name="sequence">0</field>
            </record>
        </field>
        <field name="line_ids">
            <record id="account_reports.aged_receivable_line" model="account.report.line">
                <field name="expression_ids">
                    <record id="aged_receivable_line_po_number_ref" model="account.report.expression">
                        <field name="label">po_number_ref</field>
                        <field name="engine">custom</field>
                        <field name="formula">_report_custom_engine_aged_receivable</field>
                        <field name="subformula">po_number_ref_reference</field>
                        <field name="auditable" eval="False"/>
                    </record>
                </field>
            </record>
        </field>
    </record>
</odoo>